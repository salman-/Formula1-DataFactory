							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": "/Repos/salman.lashkarara@diconium.com/Formula1-Databricks/notebooks/ingestions/ingest_driver"
						},
						"linkedServiceName": {
							"referenceName": "linked_service_databricks",
							"type": "LinkedServiceReference"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": [],
				"lastPublishTime": "2022-12-15T14:18:10Z"
