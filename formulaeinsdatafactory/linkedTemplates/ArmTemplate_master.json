{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "formulaeinsdatafactory"
		},
		"linked_service_sql_database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linked_service_sql_database'"
		},
		"linked_service_process_data_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://formuleinsstorage.blob.core.windows.net/"
		},
		"linked_service_raw_data_csv_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://formuleinsstorage.blob.core.windows.net/"
		},
		"linked_service_remote_formula1_database_csv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://ergast.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "formulaeinsdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linked_service_sql_database_connectionString": {
						"value": "[parameters('linked_service_sql_database_connectionString')]"
					},
					"linked_service_process_data_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('linked_service_process_data_properties_typeProperties_serviceEndpoint')]"
					},
					"linked_service_raw_data_csv_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('linked_service_raw_data_csv_properties_typeProperties_serviceEndpoint')]"
					},
					"linked_service_remote_formula1_database_csv_properties_typeProperties_url": {
						"value": "[parameters('linked_service_remote_formula1_database_csv_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}